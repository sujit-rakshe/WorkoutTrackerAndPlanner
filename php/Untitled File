UPDATE routineworkouts
SET workout_id = 
    CASE sequence
        WHEN 1 THEN 6
        WHEN 2 THEN 11
        WHEN 3 THEN 6
        WHEN 4 THEN 11
        WHEN 5 THEN 6
        WHEN 6 THEN 11
        WHEN 7 THEN 11
        ELSE workout_id
    END
WHERE routine_id = 3;

UPDATE routineworkouts
SET workout_id = 
    CASE sequence
        WHEN 1 THEN 1
        WHEN 2 THEN 11
        WHEN 3 THEN 1
        WHEN 4 THEN 11
        WHEN 5 THEN 1
        WHEN 6 THEN 11
        WHEN 7 THEN 11
        ELSE workout_id
    END
WHERE routine_id = 4;

